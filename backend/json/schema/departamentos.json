{
	"$schema": "http://json-schema.org/draft-07/schema#",
	"title": "Departamentos",
	"type": "object",
	"properties": {
		"departamentos": {
			"type": "array",
			"items": "object",
			"properties": {
				"centroide": {
					"type": "object",
					"properties": {
						"lat": {
							"type": "numeric",
							"minimun": -90,
							"maximun": 90
						},
						"lon": {
							"type": "numeric",
							"minimun": -90,
							"maximun": 90
						}
					},
				"id": "string",
				"nombre": "string",
				"provincia": "object",
				"properties": {
					"id": "string",
					"nombre": "string"
				}
				}
			}
		}
	}
}